-- utils

-- Writes a text centered in the screen
-- Watch out! it's not taking in account multiple lines!
function write_centered_text(text, centered)
    
    local SPRITE_WIDTH = 4
    local SPRITE_HEIGHT = 8
    local x = 0
    local y = 0

    if centered then 
        x = (SCREEN_SIZE - #text * SPRITE_WIDTH) / 2
        y = (SCREEN_SIZE - SPRITE_HEIGHT) / 2
    end    

    print(text, x, y)
end



